package euler_1;

public class Euler_1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int result = 0;
		for(int a=0;a<1000;a++)
		{
			for(int b=0;b<1000;b++)
			{
				if((a<b<(1000-a-b)) && (a^2+b^2)==((1000-a-b)^2))
				{
					result = (a*b*(1000-a-b));
				}
			}
		}
		System.out.println(result);
	}

}
